        </div>
      </b:if>
        <b:if cond='data:view.isError'><div class='errorPage'><h3>404</h3><h4><data:messages.theresNothingHere/></h4><p><data:navMessage/></p><a class='homepage' expr:href='data:blog.homepageUrl'><i class='fa fa-home'/><data:messages.home/></a></div></b:if>
    </b:includable>
